}

.row {
    position: sticky;
    top: 0%;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: black;
    padding: 0 1rem;
    transition: 0.3s;
    height: auto;
    position: fixed;
    z-index: 10000;
    transition: all 0.5s ease-in-out;
    height: auto;
    //margin-top: 0.5rem;
    text-align: center;

}

.row.active {
    background: black;
    -webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
    box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
}

.logo-name {

    color: lightgray;
    text-decoration: none;
    text-transform: uppercase;
    font-family: 'Arial';
    background: transparent;
    justify-content: center;
    white-space: nowrap;
    font-size: 1rem;
    position: absolute;
    top: 1.6rem;

}

.logo {
    text-decoration: none;
}

.menu li .active {
    color: skyblue;


}

